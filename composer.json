{
    "name": "obiz-concept/alice-generator-bundle",
    "description": "Symfony bundle for generating Alice fixtures from Doctrine entities",
    "keywords": ["fixture", "data", "test", "orm", "doctrine"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeff Way",
            "email": "jeff.way@me.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "obiz-concept/alice-generator": "dev-master",
        "doctrine/orm": "~2.4",
        "doctrine/doctrine-bundle": "^2",
        "symfony/finder": "^3.0||^4.0||^5.0||^6.0||^7.0",
        "symfony/validator": "^3.0||^4.0||^5.0||^6.0||^7.0",
        "symfony/console": "^3.0||^4.0||^5.0||^6.0||^7.0",
        "symfony/framework-bundle": "^3.0||^4.0||^5.0||^6.0||^7.0",
        "symfony/yaml": "^3.0||^4.0||^5.0||^6.0||^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0||^10.0||^11.0",
        "symfony/phpunit-bridge": "^3.0||^4.0",
        "matthiasnoback/symfony-dependency-injection-test": "^3.0||^5.1"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Obiz-concept/alice-generator"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": { "Trappar\\AliceGeneratorBundle\\": "src/Trappar/AliceGeneratorBundle" }
    }
}
